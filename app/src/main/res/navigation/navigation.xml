<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/movieListFragment">

    <fragment
        android:id="@+id/movieListFragment"
        android:name="com.example.one.MovieListFragment"
        android:label="fragment_movie_list"
        tools:layout="@layout/fragment_movie_list">
        <action
            android:id="@+id/action_movieListFragment_to_singleMovieFragment"
            app:destination="@id/singleMovieFragment" />
        <deepLink
            android:id="@+id/deepLink"
            app:uri="www.google.com" />
    </fragment>
    <fragment
        android:id="@+id/singleMovieFragment"
        android:name="com.example.one.SingleMovieFragment"
        android:label="SingleMovieFragment" >
        <argument
            android:name="movie"
            app:argType="com.example.one.MovieData" />
    </fragment>
</navigation>